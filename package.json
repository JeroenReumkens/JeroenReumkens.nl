    "motion": "^10.14.2",
    "next": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash.escape": "^4.0.7",
    "@types/node": "18.11.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-next": "^13.0.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
